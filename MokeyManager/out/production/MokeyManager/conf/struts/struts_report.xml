<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="report" extends="struts-default">
		<action name="reportDownload!*" method="{1}" class="reportDownload">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		<action name="statisticsAction!*" method="{1}" class="statisticsAction">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
	</package>

</struts>
