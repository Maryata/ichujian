    public  String getString(String[] strs) {
        StringBuffer sf = new StringBuffer();
        for (int i = 0; i < strs.length; i++) {
            sf.append(strs[i]).append(",");
        }
        return sf.deleteCharAt(sf.length() - 1).toString();
    }

    
