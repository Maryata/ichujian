/* notice message pane of info */
.carea_notice_info {
	visibility:hidden;
	float:right;
	margin:0 1px 0 3px;
	padding:2px;
	border:none;
	font-weight:bold;
	font-size:12px;
	color:white;
	background-color:blue;
	cursor:default;
}
/* notice message pane of success */
.carea_notice_good {
	visibility:hidden;
	float:right;
	margin:0 1px 0 3px;
	padding:2px;
	border:none;
	font-weight:bold;
	font-size:12px;
	color:white;
	background-color:#339900;
	cursor:default;
}
/* notice message pane of failure */
.carea_notice_bad {
	visibility:hidden;
	float:right;
	margin:0 1px 0 3px;
	padding:2px;
	border:none;
	font-weight:bold;
	font-size:12px;
	color:white;
	background-color:#EE5500;
	cursor:default;
}
