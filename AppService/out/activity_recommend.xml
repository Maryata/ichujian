<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC   
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="recommend" extends="struts-default">

		<action name="follow" method="RecommendFollow" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<action name="main" method="RecommendMain" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<action name="mainmin" method="RecommendMainMin" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<action name="hot" method="RecommendMainHot" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<action name="mainrecommend" method="RecommendMainPage" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<action name="citylist" method="GetCityList" class="Recommend">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		
	</package>
</struts>
