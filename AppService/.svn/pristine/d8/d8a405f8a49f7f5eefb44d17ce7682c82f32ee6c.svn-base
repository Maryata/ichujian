<?xml version="1.0" encoding="UTF-8"?>
<beans default-autowire="byName"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<bean id="useActivityService" class="com.sys.service.UseActivityServiceImpl" scope="prototype"></bean>
	<bean id="UseActivityAction" class="com.sys.action.UseActivityAction" scope="prototype">
		<property name="useActivityService">
			<ref bean="useActivityService"/>
		</property>
	</bean>
	
</beans>