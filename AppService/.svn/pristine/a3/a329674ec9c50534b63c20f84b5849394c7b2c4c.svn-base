<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC   
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="exchange" namespace="/exchange" extends="struts-default">
		<!-- 根据合集id获取单个游戏合集 -->
		<action name="success" class="exchangeAction"></action>
		<action name="record" method="record" class="exchangeAction"></action>
		<!--H5支付-->
		<action name="submitOrder_H5" method="submitOrder_H5" class="exchangeAction">
			<result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>

	</package>
</struts>
