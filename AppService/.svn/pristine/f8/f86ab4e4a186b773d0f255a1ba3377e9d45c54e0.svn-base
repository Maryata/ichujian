<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC   
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="ekeyShop" namespace="/ekeyShop" extends="struts-default">
		<!--签到奖励列表-->
		<action name="shopSign" method="shopSign" class="shopAction"></action>
		<!--我的任务-->
		<action name="task" method="task" class="shopAction"></action>
		<!--分享赢好礼-->
		<action name="share" method="share" class="shopAction"></action>
		<!--邀请详情-->
		<action name="inviteDetail" method="inviteDetail" class="shopAction"></action>
		<!--商品列表-->
		<action name="commodity" method="commodity" class="shopAction"></action>
		<!--积分头像-->
		<action name="integral" method="integral" class="shopAction"></action>
		<!--商品详情-->
		<action name="commodityDetail" method="commodityDetail" class="shopAction"></action>
		<!--积分获取记录-->
		<action name="gainRecords" method="gainRecords" class="shopAction"></action>
		<!--积分使用记录-->
		<action name="useRecords" method="useRecords" class="shopAction"></action>
		<!-- 商品兑换 -->
		<action name="exchangeList" method="exchangeList" class="shopAction"></action>
		<!-- 商品首页广告 -->
		<action name="advert" method="advert" class="shopAction"></action>
		<!-- 跳转到订单页 -->
		<action name="matterToOrder" method="matterToOrder" class="shopAction"></action>
		<!-- 生成订单-虚拟商品 -->
		<action name="virtualOrder" method="virtualOrder" class="shopAction"></action>
		<!-- 实物兑换结果 -->
		<action name="changeResult" method="changeResult" class="shopAction"></action>
		<!-- 支付宝回调测试 -->
		<action name="test" method="test" class="shopAction"></action>
	</package>
</struts>
