<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC   
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   <package name="useactivity" extends="struts-default">
		<!--选择记录 -->
		<action name="choose" method="ChooseInfo" class="UseActivityAction">
		    <result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
		
		<!--查看记录 -->
		<action name="select" method="SelectInfo" class="UseActivityAction">
		    <result name="input">/JsonData.jsp</result>
			<result name="success">/JsonData.jsp</result>
		</action>
			
	</package>
</struts>
