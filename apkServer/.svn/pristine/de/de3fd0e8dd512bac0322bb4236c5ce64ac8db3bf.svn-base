body{
background: #f5f5f5;
background-size: 100% 100%;
text-align:center;
margin: 0;
margin-top:10%;
font-size: 12px; 
line-height: 100%; 
font-family: Arial, sans-serif; 
}

#center{
margin:0px auto;
}

a:hover{
	color: #FFFFFF;
}
a:focus{
	color: #FFFFFF;
}
.head{
	text-align: center;
	padding-top: 10px;
	position: relative;
}
.value{
	font-size: 14px;
	color: #a3a3a3;
	margin-top: 0px;
}

/*common*/
.paddingTop0{
	padding-top: 0px;
}
.shadow{
	width: 65%;
	height: 100%;
	display: inline-block;
}
.shadow:hover{
	background: #4d83db;
}
.marginBottom5{
	padding-bottom: 5px;
}
.marginTop15{
	margin-top: 15px;
}
.tanceng{
	background: #000;
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	opacity: 0.8;
	z-index: 100;
}
