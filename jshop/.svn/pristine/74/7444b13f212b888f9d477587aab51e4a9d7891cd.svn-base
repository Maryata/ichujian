<?xml version="1.0" encoding="GBK"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><!-- create by autoCode,v1.0.0 --><mapper namespace="front.dataCenter">	<select id="front.dataCenter.selectPageList" parameterType="net.jeeshop.services.front.dataCenter.bean.DataCenter" resultType="net.jeeshop.services.front.dataCenter.bean.DataCenter">	 select * from t_datacenter where status=1	 <if test="dicId!=null and dicId!=''">	 	and dicId=#{dicId}	 </if>	 <if test="title!=null and title!=''">	 	and title=#{title}	 </if> order by order1 asc ,id desc  limit #{offset},#{pageSize} 	</select> 	<select id="front.dataCenter.selectPageCount" parameterType="net.jeeshop.services.front.dataCenter.bean.DataCenter" resultType="java.lang.Integer">	 select count(*) from t_datacenter where status=1	  <if test="dicId!=null and dicId!=''">	 	and dicId=#{dicId}	 </if>	 <if test="title!=null and title!=''">	 	and title=#{title}	 </if>	</select>		<select id="front.dataCenter.selectOne" parameterType="net.jeeshop.services.front.dataCenter.bean.DataCenter" resultType="net.jeeshop.services.front.dataCenter.bean.DataCenter">	 select * from t_datacenter where id=#{id}	</select>		<!-- 查询数据文件 -->	<select id="front.dataCenter.selectDataFiles" parameterType="net.jeeshop.services.front.dataCenter.bean.DataFile" resultType="net.jeeshop.services.front.dataCenter.bean.DataFile">	 select * from t_datafile t where 1=1	 <if test="dicId!=null and dicId!=''">	 	and exists(select 1 from t_datacenter c where c.dicId=#{dicId} and t.cid=c.id )	 </if>	 <if test="cId!=null and cId!=''">	 	and t.cId=#{cId}	 </if>	</select>		<!-- 用户申请资料 -->	<insert id="front.dataCenter.insertDataApply" parameterType="net.jeeshop.services.front.dataCenter.bean.DataApply" useGeneratedKeys="true" keyProperty="id">	  insert into t_dataapply		( id,accountId,cId,fId,createDate )		values (id,#{accountId},#{cId},#{fId},now())	</insert>	</mapper>