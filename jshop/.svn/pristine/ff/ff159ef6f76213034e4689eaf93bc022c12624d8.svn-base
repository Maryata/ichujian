package net.jeeshop.services.front.orderSerialNum.bean;

import java.io.Serializable;

import net.jeeshop.core.dao.QueryModel;

/**
 * 订单-用户-日期-顺序号
 * @author Maryn
 *
 */
public class OrderSerialNum extends QueryModel  implements Serializable{

	private static final long serialVersionUID = 1L;

	private String id;
	
	private String uid;// 用户id
	
	private String datetime;// 日期（ 年月日共六位，如：151022）
	
	private String orderSeq;// 订单顺序号
	
	private String ucode;// 用户工号

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public String getDatetime() {
		return datetime;
	}

	public void setDatetime(String datetime) {
		this.datetime = datetime;
	}

	public String getOrderSeq() {
		return orderSeq;
	}

	public void setOrderSeq(String orderSeq) {
		this.orderSeq = orderSeq;
	}

	public String getUcode() {
		return ucode;
	}

	public void setUcode(String ucode) {
		this.ucode = ucode;
	}
	
}
